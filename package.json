{
  "name": "@genx/jes",
  "version": "1.0.2",
  "description": "JSON Expression Syntax (JES)",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "doc": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './docs' -r --verbose ./src",
    "bulid:clean": "rm -rf lib",
    "test": "mocha --recursive src/**/*.spec.js",
    "cover": "nyc --reporter=html --reporter=text mocha --recursive src/**/*.spec.js",
    "build": "npm run bulid:clean && NODE_ENV=development babel src -d lib --copy-files && del lib/**/__test__",
    "build:prod": "npm run bulid:clean && NODE_ENV=production babel src -d lib --copy-files && del lib/**/__test__"
  },
  "author": "Rockie Guo <rockie@kitmi.com.au>",
  "license": "MIT",
  "repository": "https://github.com/genx-tech/gx-jes.git",
  "bugs": {
    "url": "https://github.com/genx-tech/gx-jes/issues"
  },
  "keywords": [
    "gen-x",
    "json",
    "expression"
  ],
  "mocha": {
    "require": [
      "@babel/register",
      "should"
    ],
    "timeout": 30000
  },
  "dependencies": {
    "@genx/error": "^1.0.2"
  },
  "devDependencies": {
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.6"
  }
}
